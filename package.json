{
  "name": "components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node --esm server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "cross-env NODE_ENV=production node server",
    "pret": "prettier --write src/",
    "lint": "eslint --ext .ts,.tsx src/",
    "test:unit": "vitest",
    "coverage:unit": "vitest run --coverage",
    "test:e2e": "nyc instrument src && nyc npx cypress run && nyc report --reporter=text-summary",
    "coverage:e2e": "npm run dev & wait-on http://localhost:5173 && npm run test:e2e"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react-redux": "^7.1.25",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "sirv": "^2.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@redux-devtools/core": "^3.13.1",
    "@testing-library/react": "^14.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-c8": "^0.29.3",
    "@vitest/ui": "^0.30.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.10.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "react-router-dom": "^6.9.0",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.15.3",
    "vite": "^4.1.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3",
    "wait-on": "^7.0.1"
  }
}
